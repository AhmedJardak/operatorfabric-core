{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "importHelpers": true,
    "paths": {
      "@env/*": ["environments/*"],
      "@ofStore/*": ["app/store/*"],
      "@ofActions/*": ["app/store/actions/*"],
      "@ofEffects/*": ["app/store/effects/*"],
      "@ofSelectors/*": ["app/store/selectors/*"],
      "@ofStates/*": ["app/store/states/*"],
      "@ofServices/*": ["app/services/*"],
      "@ofModel/*": ["app/model/*"],
      "@tests/*": ["tests/*"]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
